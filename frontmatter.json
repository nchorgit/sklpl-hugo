{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "weight: 1
categories",
          "name": "weight: 1
categories",
          "type": "weight: 1
categories"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "content/assets/images",
  "frontMatter.content.snippets": {
    "Image snippet": {
      "description": "Image with caption",
      "body": [
        "\"frontMatter.content.snippets\": {",
        "  \"Image snippet\": {",
        "    \"description\": \"Image with caption\",",
        "    \"body\": \"{{< caption \\\"[[&mediaUrl]]\\\" \\\"[[caption]]\\\" >}}\",",
        "    \"isMediaSnippet\": true",
        "  }",
        "}"
      ],
      "isMediaSnippet": true,
      "fields": [
        {
          "name": "className",
          "title": "Class name",
          "type": "string",
          "default": "image__caption"
        }
      ]
    }
  },
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    }
  ],
